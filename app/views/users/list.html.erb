 <table class="table">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Gender</th>
        <th>Country</th>
      </tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
          <tr>
            <td>
              <%= user.first_name %>
            </td>
            <td>
              <%= user.last_name %>
            </td>
            <td>
              <%= user.age %>
            </td>
            <td>
              <%= user.gender %>
            </td>
            <td>
              <%= user.country %>
            </td>
          </tr>
      <% end %>
    </tbody>
  </table>